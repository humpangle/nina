{
  "name": "@nina/frontend",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "repository": {
    "type": "github",
    "url": "https://github.com/kanmii/nina"
  },
  "scripts": {
    "build": "yarn remove-folders && gatsby build",
    "build:staging": "yarn remove-folders && env-cmd staging yarn build",
    "build:staging-test": "yarn remove-folders && env-cmd staging-test gatsby build",
    "build:production": "yarn remove-folders && env-cmd production yarn build",
    "develop": "yarn remove-folders && env-cmd dev gatsby develop --port 3987",
    "develop:e2e": "yarn remove-folders && env-cmd staging-test gatsby develop --port 3985",
    "start": "yarn develop",
    "start-build-staging": "yarn build:staging && yarn start:staging",
    "start:staging": "serve --no-clipboard --listen 3985 public",
    "prestart:staging": "yarn build:staging-test",
    "generate": "graphql-codegen --config codegen.yml",
    "gen-gatsby-types": "apollo codegen:generate --endpoint=http://localhost:3987/___graphql --tagName=graphql --target=typescript --includes=src/components/**/*.ts* --outputFlat=src/graphql/gatsby-types",
    "test": "jest --clear-cache && env-cmd dev jest --watch --runInBand",
    "test:coverage": "jest --clear-cache && env-cmd dev jest --coverage --runInBand",
    "test-e2e-run-staging": "start-server-and-test start:staging :3985 cy-run-staging",
    "test-e2e-open-staging": "start-server-and-test start:staging :3985 cy-open-staging",
    "test-e2e-open-dev": "start-server-and-test develop:e2e :3985 cy-open-staging",
    "cy-open-staging": "env-cmd staging-test cypress open --config baseUrl=http://localhost:3985/",
    "cy-run-staging": "env-cmd staging-test cypress run --config baseUrl=http://localhost:3985/",
    "remove-folders": "rimraf public && rimraf .cache"
  },
  "dependencies": {
    "@nina/common": "0.0.1",
    "@nina/typeorm": "0.0.1",
    "apollo-boost": "0.3.1",
    "apollo-cache-persist": "0.1.1",
    "classnames": "2.2.6",
    "env-cmd": "8.0.2",
    "formik": "1.5.2",
    "gatsby": "2.3.25",
    "gatsby-image": "2.0.39",
    "gatsby-plugin-alias-imports": "1.0.5",
    "gatsby-plugin-create-client-paths": "2.0.5",
    "gatsby-plugin-env-variables": "1.0.1",
    "gatsby-plugin-manifest": "2.0.29",
    "gatsby-plugin-netlify": "2.0.14",
    "gatsby-plugin-offline": "2.0.25",
    "gatsby-plugin-react-helmet": "3.0.12",
    "gatsby-plugin-sass": "2.0.11",
    "gatsby-plugin-sharp": "2.0.35",
    "gatsby-plugin-typescript": "2.0.12",
    "gatsby-source-filesystem": "2.0.29",
    "gatsby-transformer-sharp": "2.1.18",
    "isomorphic-fetch": "2.2.1",
    "lodash-es": "4.17.11",
    "react": "16.9.0-alpha.0",
    "react-apollo": "2.5.4",
    "react-dom": "16.9.0-alpha.0",
    "react-helmet-async": "1.0.2",
    "react-transition-group": "4.0.0",
    "semantic-ui-less": "2.4.1",
    "semantic-ui-react": "0.86.0",
    "winston": "3.2.1",
    "winston-transport-browserconsole": "1.0.1",
    "yup": "0.27.0"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "4.0.3",
    "@graphql-codegen/cli": "1.1.0",
    "@graphql-codegen/typescript": "1.1.0",
    "@graphql-codegen/typescript-operations": "1.1.0",
    "@types/classnames": "2.2.7",
    "@types/graphql": "14.2.0",
    "@types/jest": "24.0.11",
    "@types/lodash": "4.14.123",
    "@types/reach__router": "1.2.4",
    "@types/react": "16.8.14",
    "@types/react-dom": "16.8.4",
    "@types/react-transition-group": "2.9.0",
    "@types/yup": "0.26.12",
    "apollo": "2.8.3",
    "babel-preset-gatsby": "0.1.11",
    "cypress": "3.2.0",
    "cypress-file-upload": "3.1.0",
    "cypress-testing-library": "2.4.0",
    "eslint-plugin-graphql": "3.0.3",
    "gatsby-plugin-less": "2.0.12",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.7.1",
    "jest-dom": "3.1.3",
    "less": "3.9.0",
    "node-sass": "4.11.0",
    "prettier": "1.17.0",
    "react-testing-library": "6.1.2",
    "rimraf": "2.6.3",
    "serve": "11.0.0",
    "start-server-and-test": "1.7.13",
    "ts-jest": "24.0.2",
    "ts-loader": "5.3.3",
    "ts-node": "8.1.0",
    "tslint": "5.16.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-react": "4.0.0",
    "typescript": "3.4.4"
  }
}
