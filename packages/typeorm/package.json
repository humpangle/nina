{
  "name": "@nina/typeorm",
  "version": "0.0.1",
  "private": true,
  "main": "dist/index.js",
  "license": "MIT",
  "repository": {
    "type": "github",
    "url": "https://github.com/kanmii/nina"
  },
  "scripts": {
    "start": "tsc -w",
    "prestart": "rimraf dist",
    "build": "tsc",
    "prebuild": "rimraf dist",
    "typeorm:cli": "ts-node ../../node_modules/typeorm/cli.js --config ./src/ormconfig.ts",
    "typeorm:cli:js": "typeorm --config ./dist/ormconfig.js",
    "test:js": "yarn build && env-cmd staging-test jest --config jest.config.staging.js --runInBand",
    "test:watch": "env-cmd test jest --config jest.config.dev.js --runInBand --watch",
    "test:coverage": "env-cmd test jest --config jest.config.dev.js --runInBand --coverage",
    "run:migrations": "yarn typeorm:cli migration:run",
    "run:migrations:js": "yarn typeorm:cli:js migration:run",
    "make:migration": "env-cmd dev yarn typeorm:cli migration:generate -n",
    "revert:migration": "yarn typeorm:cli migration:revert"
  },
  "dependencies": {
    "@nina/common": "0.0.1",
    "pg": "7.10.0",
    "reflect-metadata": "0.1.13",
    "tslib": "1.9.3",
    "typeorm": "0.2.16"
  },
  "devDependencies": {
    "env-cmd": "8.0.2",
    "jest": "24.7.1",
    "rimraf": "2.6.3",
    "ts-jest": "24.0.2",
    "ts-node": "8.0.3",
    "tslint": "5.15.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.4.3"
  }
}
