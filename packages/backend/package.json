{
  "name": "@nina/backend",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "repository": {
    "type": "github",
    "url": "https://github.com/kanmii/nina"
  },
  "scripts": {
    "start": "yarn typeorm migration:run && node dist/index.js",
    "start:all": "yarn start:dev",
    "start:dev": "nodemon --verbose --exec \"yarn dev\" ",
    "start:debug": "nodemon --exec \"yarn debug\" ",
    "start:staging": "yarn build && env-cmd .env-staging yarn run:migrations && env-cmd .env-staging node dist/index.js",
    "dev": "env-cmd .env-dev ts-node src/index.ts",
    "predev": "env-cmd .env-dev yarn run:migrations",
    "debug": "env-cmd .env-dev node --inspect --debug-brk -r ts-node/register src/index.ts",
    "build": "tsc",
    "prebuild": "rimraf dist",
    "typeorm": "ts-node ../../node_modules/typeorm/cli.js --config ./src/ormconfig.js",
    "test": "env-cmd .env-test jest --runInBand --watch",
    "test:coverage": "env-cmd .env-test jest --runInBand --coverage",
    "run:migrations": "yarn typeorm migration:run",
    "make:migration": "env-cmd .env-dev yarn typeorm migration:generate -n",
    "revert:migration": "yarn typeorm migration:revert"
  },
  "dependencies": {
    "@nina/common": "0.0.1",
    "apollo-link": "1.2.11",
    "apollo-link-http": "1.5.14",
    "apollo-link-ws": "1.0.17",
    "apollo-server-express": "2.4.8",
    "bcrypt-nodejs": "0.0.3",
    "cors": "2.8.5",
    "express": "4.16.4",
    "graphql": "14.2.1",
    "graphql-import": "0.7.1",
    "graphql-iso-date": "3.6.1",
    "graphql-resolvers": "0.3.2",
    "graphql-s2s": "0.19.2",
    "graphql-tag": "2.10.1",
    "jsonwebtoken": "8.5.1",
    "lodash-es": "4.17.11",
    "morgan": "1.9.1",
    "node-fetch": "2.3.0",
    "node-graceful": "1.0.1",
    "pg": "7.9.0",
    "reflect-metadata": "0.1.13",
    "subscriptions-transport-ws": "0.9.16",
    "tslib": "1.9.3",
    "typeorm": "0.2.16",
    "winston": "3.2.1",
    "ws": "6.2.1",
    "yup": "0.27.0"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/graphql": "14.2.0",
    "@types/graphql-resolvers": "0.2.0",
    "@types/jest": "24.0.11",
    "@types/jsonwebtoken": "8.3.2",
    "@types/lodash": "4.14.123",
    "@types/morgan": "1.7.35",
    "@types/node-fetch": "2.3.2",
    "@types/yup": "0.26.12",
    "apollo-server-testing": "2.4.8",
    "env-cmd": "8.0.2",
    "jest": "24.7.1",
    "nodemon": "1.18.11",
    "rimraf": "2.6.3",
    "shelljs": "0.8.3",
    "ts-jest": "24.0.2",
    "ts-node": "8.0.3",
    "tslint": "5.15.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.4.3"
  },
  "nodemonConfig": {
    "ignore": [
      "src/__tests__/",
      "src/migration/"
    ],
    "watch": [
      "src"
    ],
    "ext": ".ts"
  },
  "private": true
}
