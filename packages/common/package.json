    "@graphql-codegen/typescript-operations": "1.1.1",
    "@graphql-codegen/typescript-resolvers": "1.1.1",
    "@types/yup": "0.26.12",
    "chokidar": "2.1.5",
    "concurrently": "4.1.0",
    "graphql-s2s": "0.19.2",
    "prettier": "1.17.0",
    "tslint": "5.16.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.4.5"
  }
